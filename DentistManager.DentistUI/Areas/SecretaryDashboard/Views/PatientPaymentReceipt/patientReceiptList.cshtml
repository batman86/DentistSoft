@model IEnumerable<DentistManager.Domain.ViewModel.PaymentReceiptPresentViewModel>

@{
    ViewBag.Title = "patientReceiptList";
}

<h2>patientReceiptList</h2>

<p>
    @Html.ActionLink("Create New", "patientReceiptCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.receiptAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReviceDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.reciverName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.receiptAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReviceDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.reciverName)
        </td>
        <td>
            @Html.ActionLink("Details", "patientReceiptDetails", new { receiptID = item.receiptID }) |
            @Html.ActionLink("Delete", "patientReceiptDelete", new { receiptID = item.receiptID })
        </td>
    </tr>
}

</table>
